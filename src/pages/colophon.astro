---
import { getEntry } from 'astro:content'
import PageLayout from '../layouts/PageLayout.astro'
import EmailDisplay from '../components/EmailDisplay.svelte'

const entry = await getEntry('pages', 'colophon')
const { Content } = await entry.render()
const pageInfo = {
  title: entry.data.title,
}
---

<PageLayout {pageInfo}>
  <Content />
  <EmailDisplay client:only />
</PageLayout>
