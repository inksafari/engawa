---
import { feedUrls } from '../../consts.ts'
import PageLayout from '../../layouts/PageLayout.astro'

const pageInfo = {
  title: 'RSS Feeds',
}
---

<PageLayout {pageInfo}>
  <ul>
    <li>
      ATOM格式： <a href={feedUrls.atom} rel='prefetch-intent'>feed.xml</a>
    </li>
    <li>
      JSON格式： <a href={feedUrls.json} rel='prefetch-intent'>feed.json</a>
    </li>
  </ul>
</PageLayout>
