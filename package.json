{
  "name": "@inksafari/engawa",
  "description": "Personal blog built with Astro and svelte components.",
  "version": "0.0.0-development",
  "private": true,
  "homepage": "https://codeberg.org/inksafari/engawa",
  "scripts": {
    "postinstall": "astro telemetry disable",
    "===== development =====": "",
    "new": "bun --bun ./scripts/new.mjs",
    "mod": "bun --bun ./scripts/get-updatetime.mjs",
    "clear": "bun --bun ./scripts/clear.mjs",
    "dev": "astro dev --config astro.config.dev.mjs",
    "===== linting & formatting =====": "",
    "code:diff": "git diff --exit-code > temp.diff",
    "code:biome": "bunx @biomejs/biome check .",
    "fmt": "bunx @biomejs/biome check --write .",
    "find:keys": "gitleaks detect --source . -v",
    "lint": "run-p lint:lefthook lint:ls",
    "lint:lefthook": "lefthook run lint",
    "lint:ls": "ls-lint",
    "css:lint": "stylelint 'src/**/*.css' --config .stylelint.config.mjs --formatter string --output-file node_modules/.cache/css_report.txt",
    "css:fix": "stylelint 'src/**/*.css' --config .stylelint.config.mjs --allow-empty-input --cache --cache-strategy content --cache-location node_modules/.cache/.stylelintcache --fix",
    "text:redpen": "vale sync; vale src/content/**/*.mdx",
    "commit:lint": "commitlint --from HEAD~1 --to HEAD --verbose",
    "===== testing =====": "",
    "test:playwright": "playwright test",
    "test:playwright:report": "playwright show-report test/reports",
    "test:e2e": "run-s test:playwright test:playwright:report",
    "===== building =====": "",
    "cjk": "sh ./fontSubset.sh",
    "prebuild": "run-p cjk",
    "build:dev": "MODE=dev astro build",
    "build:prod": "MODE=production astro build",
    "build:headers": "bun run ./scripts/gen-header.js",
    "preview": "astro preview",
    "serve": "run-s build:dev build:headers preview",
    "ss:prod": "run-s build:prod build:headers preview",
    "===== deployment =====": "",
    "staging": "run-s build:prod pages:staging:local",
    "pages:staging:local": "wrangler pages dev",
    "firebase:staging:local": "firebase emulators:start",
    "firebase:staging:remote": "firebase hosting:channel:deploy yan --expires 10m",
    "deploy:firebase": "firebase deploy --only hosting",
    "deploy:pages": "wrangler pages deploy",
    "deploy": "run-s prebuild build:prod build:headers deploy:pages"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^11.0.1",
    "@astrojs/mdx": "^2.3.1",
    "@astrojs/svelte": "^5.7.0",
    "@expressive-code/plugin-collapsible-sections": "^0.33.5",
    "@expressive-code/plugin-line-numbers": "^0.33.5",
    "@extractus/oembed-extractor": "^4.0.5",
    "@fec/remark-a11y-emoji": "^4.0.2",
    "@iconify-json/lucide": "^1.1.203",
    "@kindspells/astro-shield": "^1.3.6",
    "@pagefind/default-ui": "^1.1.0",
    "@playform/compress": "^0.0.3",
    "@playform/inline": "^0.1.0",
    "@swup/astro": "^1.4.1",
    "astro": "^4.12.2",
    "astro-capo": "^0.0.1",
    "astro-icon": "^1.1.0",
    "astro-purgecss": "^4.1.1",
    "budoux": "^0.6.2",
    "defu": "^6.1.4",
    "fontaine": "^0.5.0",
    "hast-util-select": "^6.0.2",
    "hastscript": "^9.0.0",
    "lightningcss": "^1.25.1",
    "limax": "^4.1.0",
    "lunarphase-js": "^2.0.3",
    "mdast-util-to-hast": "^13.2.0",
    "mdast-util-to-string": "^4.0.0",
    "pagefind": "^1.1.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-citation": "^2.0.0",
    "rehype-color-chips": "^0.1.3",
    "rehype-components": "^0.3.0",
    "rehype-katex": "^7.0.0",
    "rehype-raw": "^7.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-directive": "^3.0.0",
    "remark-emoji": "^4.0.1",
    "remark-expressive-code": "^0.33.5",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.0",
    "sanitize-html": "^2.13.0",
    "sirv": "^2.0.4",
    "spacetime": "^7.6.0",
    "svelte": "^4.2.18",
    "unified": "^11.0.5",
    "uniqolor": "^1.1.1",
    "unist-builder": "^4.0.0",
    "unist-util-is": "^6.0.0",
    "unist-util-visit": "^5.0.0",
    "xast-util-feed": "^2.0.0",
    "xast-util-to-xml": "^4.0.0",
    "xastscript": "^4.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@guidepup/guidepup": "^0.22.3",
    "@guidepup/playwright": "^0.14.0",
    "@iconify-json/fa6-brands": "^1.1.21",
    "@iconify-json/fa6-solid": "^1.1.23",
    "@ls-lint/ls-lint": "^2.2.3",
    "@playwright/test": "^1.45.3",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "astro-devtool-breakpoints": "^0.3.0",
    "astro-meta-tags": "^0.3.0",
    "browserslist": "^4.23.2",
    "browserslist-config-wikimedia": "^0.6.1",
    "bun-types": "^1.1.21",
    "dedent": "^1.5.3",
    "jsdom": "^24.1.1",
    "lefthook": "^1.7.11",
    "lost-pixel": "^3.17.0",
    "nolyfill": "^1.0.39",
    "npm-run-all": "^4.1.5",
    "ora": "^8.0.1",
    "stylelint": "^16.8.1",
    "stylelint-config-standard": "36.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-declaration-strict-value": "^1.10.6",
    "stylelint-no-unsupported-browser-features": "^8.0.1",
    "stylelint-plugin-defensive-css": "^1.0.4",
    "stylelint-value-no-unknown-custom-properties": "^6.0.1",
    "tslib": "^2.6.3",
    "typescript": "5.4.4",
    "vite": "^5.3.5",
    "wrangler": "^3.67.1"
  },
  "type": "module",
  "browserslist": [
    "extends browserslist-config-wikimedia/modern"
  ],
  "engines": {
    "node": "^18.17.1 || ^20.3.0 || >=21.0.0"
  },
  "overrides": {
    "is-core-module": "npm:@nolyfill/is-core-module@^1",
    "isarray": "npm:@nolyfill/isarray@^1",
    "string.prototype.matchall": "npm:@nolyfill/string.prototype.matchall@^1",
    "string.prototype.padend": "npm:@nolyfill/string.prototype.padend@^1",
    "typedarray": "npm:@nolyfill/typedarray@^1"
  },
  "module": "index.ts"
}
